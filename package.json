{
  "name": "astro-nano",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro sync && bunx --bun astro dev",
    "prebuild": "bun run scripts/generate-discord-verification.ts",
    "build": "bun install --force && astro sync && bunx --bun astro build",
    "preview": "bunx --bun astro preview",
    "check": "bunx --bun astro check",
    "lint": "eslint . --flag unstable_ts_config --fix && prettier --write ."
  },
  "dependencies": {
    "@astrojs/mdx": "4.0.0-beta.3",
    "@astrojs/rss": "^4.0.9",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.2",
    "@playform/inline": "^0.1.0",
    "astro": "5.0.0-beta.7",
    "astro-breadcrumbs": "^3.2.0",
    "astro-capo": "^0.0.1",
    "astro-compressor": "^0.4.1",
    "astro-purgecss": "^4.8.0",
    "purgecss": "^6.0.0",
    "sharp": "^0.33.5",
    "spotify-url-info": "^3.2.17",
    "tailwindcss": "^3.4.14",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@astrojs/check": "^0.9.4",
    "@astrojs/ts-plugin": "^1.10.4",
    "@eslint/js": "^9.14.0",
    "@iconify-json/lucide": "^1.2.13",
    "@prettier/plugin-xml": "^3.4.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/bun": "^1.1.13",
    "@types/eslint__js": "^8.42.3",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "astro-devtool-breakpoints": "^0.3.0",
    "astro-embed": "^0.7.4",
    "astro-eslint-parser": "^1.0.3",
    "astro-font": "^0.1.81",
    "astro-icon": "^1.1.2",
    "astro-link": "^1.2.1",
    "astro-opengraph-images": "^1.11.7",
    "clsx": "^2.1.1",
    "daisyui": "^4.12.14",
    "depcheck": "^1.4.7",
    "eslint": "^9.14.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "kleur": "^4.1.5",
    "lint-staged": "^15.2.10",
    "ora": "^8.1.1",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "simple-git-hooks": "^2.11.1",
    "tailwind-merge": "^2.5.4",
    "tw-colors": "^3.3.2",
    "tw-to-css": "^0.0.12",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "unplugin-auto-import": "^0.18.3"
  },
  "overrides": {
    "shiki": "latest",
    "@shikijs/core": "latest",
    "oniguruma-to-es": "latest"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run prebuild && bun run lint-staged"
  },
  "lint-staged": {
    "*.astro": "prettier --write",
    "*": "eslint --fix --flag unstable_ts_config"
  }
}
