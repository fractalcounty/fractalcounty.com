{
  "name": "astro-nano",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "prepare": "fish -c 'towebp /src/content/artwork'",
    "predev": "bun run prepare && astro sync",
    "dev": "astro dev",
    "dev:host": "astro dev --host",
    "prebuild": "bun run prepare && astro sync && astro check",
    "build": "astro build",
    "preview": "astro preview",
    "preview:host": "astro preview --host",
    "regen": "fish -c 'npmregen'",
    "lint": "eslint . --flag unstable_ts_config",
    "lint:fix": "eslint . --fix --flag unstable_ts_config"
  },
  "dependencies": {
    "@astrojs/mdx": "^3.1.7",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^5.1.1",
    "@playform/inline": "^0.1.0",
    "astro": "^4.15.11",
    "astro-breadcrumbs": "^3.2.0",
    "astro-compressor": "^0.4.1",
    "astro-purgecss": "^4.6.1",
    "purgecss": "^6.0.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@astrojs/check": "^0.9.4",
    "@eslint/js": "^9.14.0",
    "@iconify-json/solar": "^1.2.1",
    "@prettier/plugin-xml": "^3.4.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.12.2",
    "astro-embed": "^0.7.4",
    "astro-eslint-parser": "^1.0.3",
    "astro-font": "^0.1.81",
    "astro-icon": "^1.1.1",
    "astro-link": "^1.2.1",
    "astro-opengraph-images": "^1.11.3",
    "astro-seo": "^0.8.4",
    "clsx": "^2.1.1",
    "daisyui": "^4.12.12",
    "depcheck": "^1.4.7",
    "eslint": "^9.14.0",
    "eslint-plugin-astro": "^1.3.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.10",
    "react": "^18.3.1",
    "simple-git-hooks": "^2.11.1",
    "tailwind-merge": "^2.5.3",
    "tw-to-css": "^0.0.12",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run prepare && bun run predev && bun run lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
